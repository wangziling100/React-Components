{
  "name": "@wangziling100/state-manager",
  "version": "0.5.1",
  "description": "A React based state management system, like Redux, but easier to use",
  "keywords": [
    "react",
    "react component",
    "react state",
    "state manager"
  ],
  "author": "Wang Xingbo <wangziling1000@gmail.com>",
  "homepage": "https://github.com/wangziling100/React-Components/blob/master/packages/state-manager",
  "main": "./dist/index.js",
  "types": "./src/index.d.ts",
  "license": "MIT",
  "directories": {
    "lib": "lib",
    "test": "./src/test"
  },
  "files": [
    "src/*d.ts",
    "src/*d.ts.map",
    "src/lib/*d.ts",
    "src/lib/*d.ts.map",
    "dist/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangziling100/React-Components.git"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/*.test.**",
      "!**/coverage/**",
      "!**/.github/**",
      "!**/dist/**",
      "!**/stories/**",
      "!**/*config.**"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "scripts": {
    "test": "jest src/test",
    "eject": "react-scripts eject",
    "bundle": "webpack",
    "build": "webpack --mode production",
    "start": "react-scripts start",
    "lint": "eslint --ignore-path .eslintignore ./src/index.tsx ./src/lib/*",
    "test:react": "react-scripts test"
  },
  "bugs": {
    "url": "https://github.com/wangziling100/React-Components/issues"
  },
  "dependencies": {
    "@wangziling100/store-manager": "^0.4.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
