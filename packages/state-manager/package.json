    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.3",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^12.1.1",
    "@types/autoprefixer": "^9.7.2",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/postcss-import": "^12.0.0",
